{%- assign mobileMenu = linklists[section.settings.mobileMenu] -%}
{%- assign mobileMenuSecondary = linklists[section.settings.mobileMenuSecondary] -%}
{%- assign mainMenu = linklists[section.settings.menu] -%}
{%- assign headerBgColorBrightness = settings.style_header_bg_color | color_brightness -%}
{%- assign headerContentColorBrightness = settings.style_header_text_color | color_brightness -%}
{%- assign headerAccentColorBrightness = settings.style_header_accent_color | color_brightness -%}

{%- capture sectionStyle -%}
    #finqu-section-{{ section.id }} .site-header-items-container,
    #finqu-section-{{ section.id }} .site-mobile-header-container {
        --style-bg-color: {{ settings.style_header_bg_color }};
        --style-heading-color: {{ settings.style_header_text_color }};
        --style-text-color: {{ settings.style_header_text_color }};
        --style-link-color: {{ settings.style_header_text_color }};
        --style-icon-color: {{ settings.style_header_text_color }};
        --style-btn-bg-color: {{ settings.style_header_text_color }};
        --style-btn-text-color: {{ settings.style_header_bg_color }};
        --style-btn-border-color: {{ settings.style_header_text_color }};
        --style-form-placeholder-color: {{ settings.style_header_text_color }};
        --style-form-text-color: {{ settings.style_header_text_color }};
        --style-form-border-color: {{ settings.style_header_text_color }};

        --style-accent-color: {{ settings.style_header_accent_color }};

        {%- if headerAccentColorBrightness > 190 -%}
            --style-accent-text-color: {{ '#000000' | color_min_contrast: settings.style_header_accent_color }};
        {%- else -%}
            --style-accent-text-color: {{ '#ffffff' | color_min_contrast: settings.style_header_accent_color }};
        {%- endif -%}

        {%- if headerContentColorBrightness > 190 -%}
            --style-btn-bg-active-color: {{ settings.style_header_text_color | color_darken: 10 }};
            --style-btn-bg-hover-color: {{ settings.style_header_text_color | color_darken: 8 }};
            --style-btn-border-hover-color: {{ settings.style_header_text_color | color_darken: 8 }};
            --style-btn-border-active-color: {{ settings.style_header_text_color | color_darken: 13 }};
            --style-border-accent-color: {{ settings.style_header_text_color | color_darken: 50 }};
            --style-form-border-focus-color: {{ settings.style_header_text_color | color_darken: 10 }};
            --style-form-border-disabled-color: {{ settings.style_header_text_color | color_darken: 10 }};
        {%- else -%}
            --style-btn-bg-active-color: {{ settings.style_header_text_color | color_lighten: 10 }};
            --style-btn-bg-hover-color: {{ settings.style_header_text_color | color_lighten: 8 }};
            --style-btn-border-hover-color: {{ settings.style_header_text_color | color_lighten: 8 }};
            --style-btn-border-active-color: {{ settings.style_header_text_color | color_lighten: 13 }};
            --style-border-accent-color: {{ settings.style_header_text_color | color_lighten: 50 }};
            --style-form-border-focus-color: {{ settings.style_header_text_color | color_lighten: 10 }};
            --style-form-border-disabled-color: {{ settings.style_header_text_color | color_lighten: 10 }};
            --style-border-color: {{ settings.style_header_text_color | color_lighten: 10 }};
        {%- endif -%}   

        {%- if headerBgColorBrightness > 190 -%}
            --style-border-color: {{ settings.style_header_bg_color | color_darken: 15 }};
            --style-border-focus-color: {{ settings.style_header_text_color | color_darken: 20 }};
            --style-form-bg-hover-color: {{ settings.style_header_bg_color | color_darken: 5 }};
            --style-form-bg-disabled-color: {{ settings.style_header_bg_color | color_darken: 10 }};
        {%- else -%}
            --style-border-color: {{ settings.style_header_bg_color | color_lighten: 15 }};
            --style-border-focus-color: {{ settings.style_header_bg_color | color_lighten: 20 }};
            --style-form-bg-hover-color: {{ settings.style_header_bg_color | color_lighten: 5 }};
            --style-form-bg-disabled-color: {{ settings.style_header_bg_color | color_lighten: 10 }};
        {%- endif -%}

        --bs-body-bg: {{ settings.style_header_bg_color }};
        --bs-body-bg-rgb: {{ settings.style_header_bg_color | color_to_rgb | color_extract: 'rgb' | join: ', ' }};
        --bs-body-color: {{ settings.style_header_text_color }};
        --bs-body-color-rgb: {{ settings.style_header_text_color | color_to_rgb | color_extract: 'rgb' | join: ', ' }};

        --bs-secondary-bg: {{ settings.style_header_bg_color | color_darken: 30 }};
        --bs-secondary-bg-rgb: {{ settings.style_header_bg_color | color_darken: 30 | color_to_rgb | color_extract: 'rgb' | join: ', ' }};
        --bs-secondary-color: {{ settings.style_header_text_color | color_modify: 'alpha', 0.75 }};
        --bs-secondary-color-rgb: {{ settings.style_header_text_color | color_desaturate: 25 | color_to_rgb | color_extract: 'rgb' | join: ', ' }};

        --bs-tertiary-bg: {{ settings.style_header_bg_color | color_darken: 10 }};
        --bs-tertiary-bg-rgb: {{ settings.style_header_bg_color | color_darken: 10 | color_to_rgb | color_extract: 'rgb' | join: ', ' }};
        --bs-tertiary-color: {{ settings.style_header_text_color | color_modify: 'alpha', 0.5 }};
        --bs-tertiary-color-rgb: {{ settings.style_header_text_color | color_desaturate: 50 | color_to_rgb | color_extract: 'rgb' | join: ', ' }};

        --bs-emphasis-color: {{ settings.style_header_accent_color }};
        --bs-emphasis-color-rgb: {{ settings.style_header_accent_color | color_to_rgb | color_extract: 'rgb' | join: ', ' }};

        --bs-link-color: {{ settings.style_header_text_color }};
        --bs-link-color-rgb: {{ settings.style_header_text_color | color_to_rgb | color_extract: 'rgb' | join: ', ' }};
        --bs-link-hover-color: {{ settings.style_header_text_color }};
        --bs-link-hover-color-rgb: {{ settings.style_header_text_color | color_to_rgb | color_extract: 'rgb' | join: ', ' }};

        --bs-border-color: var(--style-border-color);
        --bs-heading-color: var(--style-heading-color);
    }
    #finqu-section-{{ section.id }} .site-header-container {
        --site-header-items-padding-y: {{ section.settings.headerItemsPaddingY }};
        {%- if section.settings.logoPosition == 'start' -%}
            {%- if section.settings.menuItemsAlign == 'start' -%}
                --site-header-template-columns: auto 1fr auto;
            {%- else -%}
                --site-header-template-columns: 20% 1fr 20%;
            {%- endif -%}
            --site-header-logo-position: items-start;
            --site-header-logo-items-align: start;
            --site-header-logo-margin: 0 20px 0 0;
            --site-header-nav-position: items-center;
            --site-header-nav-items-align: {{ section.settings.menuItemsAlign }};
            --site-header-actions-position: items-end;
            --site-header-actions-items-align: end;
        {%- elsif section.settings.logoPosition == 'center' -%}
            --site-header-template-columns: 1fr auto 1fr;
            --site-header-logo-position: items-center;
            --site-header-logo-items-align: center;
            --site-header-logo-margin: 0 20px;
            --site-header-nav-position: items-start;
            --site-header-nav-items-align: start;
            --site-header-actions-position: items-end;
            --site-header-actions-items-align: end;
        {%- endif -%}
    }
    #finqu-section-{{ section.id }} .site-mobile-header-container {
        {%- if section.settings.mobileLogoPosition == 'start' -%}
            --site-mobile-header-template-columns: auto 1fr auto;
            --site-mobile-header-logo-position: items-center;
            --site-mobile-header-logo-items-align: start;
            --site-mobile-header-logo-margin: 0 20px 0 0;
            --site-mobile-header-nav-position: items-start;
            --site-mobile-header-nav-items-align: start;
            --site-mobile-header-actions-position: items-end;
            --site-mobile-header-actions-items-align: end;
        {%- elsif section.settings.mobileLogoPosition == 'center' -%}
            --site-mobile-header-template-columns: 1fr auto 1fr;
            --site-mobile-header-logo-position: items-center;
            --site-mobile-header-logo-items-align: center;
            --site-mobile-header-header-logo-margin: 0 20px;
            --site-mobile-header-nav-position: items-start;
            --site-mobile-header-nav-items-align: start;
            --site-mobile-header-actions-position: items-end;
            --site-mobile-header-actions-items-align: end;
        {%- endif -%}
    }
    {%- if settings.styleCartVariant == 2 -%}

        {%- assign styleformBgColorBrightness = settings.style_form_bg_color | color_brightness -%}
        {%- assign styleformBorderColorBrightness = settings.style_form_border_color | color_brightness -%}
        {%- assign styleTextColorBrightness = settings.style_text_color | color_brightness -%}
        {%- assign styleButtonBgColorColorBrightness = settings.style_btn_bg_color | color_brightness -%}

        #finqu-section-{{ section.id }} .site-header-cart-mini-container {
            --style-bg-color: {{ settings.style_bg_color }};
            --style-heading-color: {{ settings.style_text_color }};
            --style-text-color: {{ settings.style_text_color }};
            --style-link-color: {{ settings.style_link_color }};
            --style-icon-color: {{ settings.style_icon_color }};
            --style-btn-bg-color: {{ settings.style_btn_bg_color }};
            --style-btn-text-color: {{ settings.style_btn_text_color }};
            --style-btn-border-color: {{ settings.style_btn_bg_color }};
            --style-form-placeholder-color: {{ settings.style_form_text_color | color_lighten: 15 }};
            --style-form-text-color: {{ settings.style_form_text_color }};
            --style-form-bg-color: {{ settings.style_form_bg_color }};
            --style-form-border-color: {{ settings.style_form_border_color }};
            --style-border-color: {{ settings.style_border_color }};
            --style-border-focus-color: {{ settings.style_border_color | color_lighten: 25 }};
            --style-border-accent-color: {{ settings.style_header_text_color | color_mix: settings.style_header_bg_color, 50 }};

            {%- if styleformBorderColorBrightness > 190 -%}
                --style-form-border-focus-color: {{ settings.style_form_border_color | color_darken: 10 }};
                --style-form-border-disabled-color: {{ settings.style_form_border_color | color_darken: 10 }};
            {%- else -%}
                --style-form-border-focus-color: {{ settings.style_form_border_color | color_lighten: 10 }};
                --style-form-border-disabled-color: {{ settings.style_form_border_color | color_lighten: 10 }};
            {%- endif -%}

            {%- if styleTextColorBrightness > 190 -%}
                --style-text-accent-color: {{ settings.style_text_color | color_darken: 10 }};
                --style-text-muted-color: {{ settings.style_text_color | color_darken: 30 }};
            {%- else -%}
                --style-text-accent-color: {{ settings.style_text_color | color_lighten: 10 }};
                --style-text-muted-color: {{ settings.style_text_color | color_lighten: 30 }};
            {%- endif -%}

            {%- if styleButtonBgColorColorBrightness > 190 -%}
                --style-btn-bg-active-color: {{ settings.style_btn_bg_color | color_darken: 10 }};
                --style-btn-bg-hover-color: {{ settings.style_btn_bg_color | color_darken: 8 }};
                --style-btn-border-hover-color: {{ settings.style_btn_bg_color | color_darken: 8 }};
                --style-btn-border-active-color: {{ settings.style_btn_bg_color | color_darken: 13 }};
            {%- else -%}
                --style-btn-bg-active-color: {{ settings.style_btn_bg_color | color_lighten: 10 }};
                --style-btn-bg-hover-color: {{ settings.style_btn_bg_color | color_lighten: 8 }};
                --style-btn-border-hover-color: {{ settings.style_btn_bg_color | color_lighten: 8 }};
                --style-btn-border-active-color: {{ settings.style_btn_bg_color | color_lighten: 13 }};
            {%- endif -%}

            {%- if styleformBgColorBrightness > 190 -%}
                --style-form-bg-hover-color: {{ settings.style_form_bg_color | color_darken: 5 }};
                --style-form-bg-disabled-color: {{ settings.style_form_bg_color | color_darken: 10 }};
            {%- else -%}
                --style-form-bg-hover-color: {{ settings.style_form_bg_color | color_lighten: 5 }};
                --style-form-bg-disabled-color: {{ settings.style_form_bg_color | color_lighten: 10 }};
            {%- endif -%}

            --bs-body-bg: {{ settings.style_bg_color }};
            --bs-body-bg-rgb: {{ settings.style_bg_color | color_to_rgb | color_extract: 'rgb' | join: ', ' }};
            --bs-body-color: {{ settings.style_text_color }};
            --bs-body-color-rgb: {{ settings.style_text_color | color_to_rgb | color_extract: 'rgb' | join: ', ' }};

            --bs-secondary-bg: {{ settings.style_bg_color | color_darken: 30 }};
            --bs-secondary-bg-rgb: {{ settings.style_bg_color | color_darken: 30 | color_to_rgb | color_extract: 'rgb' | join: ', ' }};
            --bs-secondary-color: {{ settings.style_ext_color | color_modify: 'alpha', 0.75 }};
            --bs-secondary-color-rgb: {{ settings.style_text_color | color_desaturate: 25 | color_to_rgb | color_extract: 'rgb' | join: ', ' }};

            --bs-tertiary-bg: {{ settings.style_bg_color | color_darken: 10 }};
            --bs-tertiary-bg-rgb: {{ settings.style_bg_color | color_darken: 10 | color_to_rgb | color_extract: 'rgb' | join: ', ' }};
            --bs-tertiary-color: {{ settings.style_text_color | color_modify: 'alpha', 0.5 }};
            --bs-tertiary-color-rgb: {{ settings.style_text_color | color_desaturate: 50 | color_to_rgb | color_extract: 'rgb' | join: ', ' }};

            --bs-emphasis-color: {{ settings.style_accent_color }};
            --bs-emphasis-color-rgb: {{ settings.style_accent_color | color_to_rgb | color_extract: 'rgb' | join: ', ' }};

            --bs-link-color: {{ settings.style_link_color }};
            --bs-link-color-rgb: {{ settings.style_link_color | color_to_rgb | color_extract: 'rgb' | join: ', ' }};
            --bs-link-hover-color: {{ settings.style_link_color }};
            --bs-link-hover-color-rgb: {{ settings.style_link_color | color_to_rgb | color_extract: 'rgb' | join: ', ' }};

            --bs-border-color: var(--style-border-color);
            --bs-heading-color: var(--style-heading-color);
        }
    {%- endif -%}
    {%- if section.settings.gridContainer == 'fluid' -%}
        #finqu-section-{{ section.id }} .site-header-container .container-fluid {
            --style-grid-container-padding-x: 3rem;
        }
    {%- endif -%}
{%- endcapture -%}

<style>{{ sectionStyle | strip | replace: '  ', '' | strip_newlines }}</style>

{%- if request.page_type == 'frontpage' -%}
    <h1 class="text-hidden">{{- store.name -}}</h1>
{%- endif -%}

{%- if settings.accessibilityShowSearch -%}

    <div class="site-search-container site-search-floating">

        <div class="site-search">

            <div class="site-search-form-container">

                {% form 'search', class: 'site-search-form' %}

                    {% if form.posted_successfully? %}

                        <script type="application/javascript">
                            document.addEventListener('DOMContentLoaded', function() {
                                document.dispatchEvent(new CustomEvent('theme:search', {
                                    detail: {
                                        query: {{ submitted_form.q | json }}
                                    }
                                }));
                            });
                        </script>

                    {% endif %}

                    <div class="input-group">

                        <button type="submit" class="btn pe-0">

                            {%- render 'icon', icon: 'search', class: 'svg-icon-fw' -%}

                        </button>

                        <input
                            class="form-control"
                            type="search"
                            placeholder="{{ 'general.search_products' | t }}"
                            name="q"
                            value="{{ search.search_term }}"
                            autocomplete="off">

                        <button type="reset" class="btn btn-icon ps-0">

                            {%- render 'icon', icon: 'close', class: 'svg-icon-fw' -%}

                        </button>

                    </div>

                {% endform %}

            </div>

            <div class="site-search-results-container">

                <div class="site-search-results"></div>

            </div>

        </div>

    </div>

{%- endif -%}

<div class="site-mobile-header-container{% if section.settings.stickyMobileHeader %} site-mobile-header-sticky{% endif %}">

    <div class="site-mobile-header">

        <div class="site-mobile-header-items-container">

            <div class="site-mobile-header-items container">

                <div class="site-mobile-header-items-nav-container">

                    <div class="site-mobile-header-item">

                        <button type="button" class="site-mobile-navigation-toggle btn btn-text" aria-label="{{ 'general.navigation' | t }}">

                            {%- render 'icon', icon: 'navigation-menu', class: 'svg-icon-fw' -%}
                            {%- render 'icon', icon: 'close', class: 'd-none' -%}

                        </button>

                    </div>

                    {%- if settings.accessibilityShowSearch and section.settings.mobileShowSearchInNavigation == false and section.settings.mobileLogoPosition == 'center' -%}

                        <div class="site-mobile-header-item site-mobile-header-item-search">

                            <button type="button" class="site-search-toggle btn btn-text" aria-label="{{ 'general.search' | t }}">

                                {%- render 'icon', icon: 'search', class: 'svg-icon-fw' -%}

                            </button>

                        </div>

                    {% elsif store.customer_accounts_enabled and settings.accessibilityShowSearch and section.settings.mobileShowSearchInNavigation == true and section.settings.mobileLogoPosition == 'center'  %}

                        <div class="site-mobile-header-item site-mobile-header-item-account">

                            <a
                                href="{{ session.logged_in | yepnope: routes.account_url, routes.account_login_url }}"
                                class="btn btn-text">

                                {%- render 'icon', icon: 'user', class: 'svg-icon-fw' -%}

                            </a>

                        </div>

                    {% endif %}

                </div>

                <div class="site-mobile-header-items-logo-container">

                    <div class="site-mobile-header-item site-mobile-header-logo">

                        <a href="{{ routes.root_url }}">

                            {%- assign imgSizeOnPhone = '200,25' -%}
                            {%- assign imgSizeOnTablet = '400,35' -%}

                            {% if section.settings.logoImg %}

                                <img
                                    alt="{{ store.name }}"
                                    src="{{ section.settings.logoImg | thumb: imgSizeOnTablet }}"
                                    srcset="{{ section.settings.logoImg | thumb: imgSizeOnTablet }} 1x, {{ section.settings.logoImg | thumb: imgSizeOnTablet | thumb:'@2x' }} 2x"
                                    class="img-fluid site-mobile-header-logo-img d-none d-md-block">

                                <img
                                    alt="{{ store.name }}"
                                    src="{{ section.settings.logoImg | thumb: imgSizeOnPhone }}"
                                    srcset="{{ section.settings.logoImg | thumb: imgSizeOnPhone }} 1x, {{ section.settings.logoImg | thumb: imgSizeOnPhone | thumb:'@2x' }} 2x"
                                    class="img-fluid site-mobile-header-logo-img d-block d-md-none">

                            {% else %}

                                <span class="site-mobile-header-logo-text">
                                    {{ store.name }}
                                </span>

                            {% endif %}

                        </a>

                    </div>

                </div>

                <div class="site-mobile-header-items-actions-container">

                    {%- if settings.accessibilityShowSearch and section.settings.mobileShowSearchInNavigation == false and section.settings.mobileLogoPosition == 'start' -%}

                        <div class="site-mobile-header-item site-mobile-header-item-search">

                            <button type="button" class="site-search-toggle btn btn-text" aria-label="{{ 'general.search' | t }}">

                                {%- render 'icon', icon: 'search', class: 'svg-icon-fw' -%}

                            </button>

                        </div>

                    {% endif %}

                    {%- if store.customer_accounts_enabled and section.settings.mobileLogoPosition == 'start' -%}

                        <div class="site-mobile-header-item site-mobile-header-item-account">

                            <a
                                href="{{ session.logged_in | yepnope: routes.account_url, routes.account_login_url }}"
                                class="btn btn-text">

                                {%- render 'icon', icon: 'user', class: 'svg-icon-fw' -%}

                            </a>

                        </div>

                    {% endif %}

                    {% if settings.accessibilityShowWishlist %}

                        <div class="site-mobile-header-item site-mobile-header-item-wishlist">

                            <a href="{{ routes.account_wishlist_url }}" class="btn btn-text" aria-label="{{ 'wishlist.title' | t }}">

                                <span class="site-mobile-header-item-counter{% if wishlist.products.size > 0 %} has-items{% endif %}">

                                    <span class="site-mobile-header-item-counter-text" data-wishlist-item-count>
                                        {{ wishlist.products.size }}
                                    </span>

                                </span>

                                {%- render 'icon', icon: 'like', class: 'svg-icon-fw' -%}

                            </a>

                        </div>

                    {% endif %}

                    <div class="site-mobile-header-item site-mobile-header-item-cart">

                        <a href="{{ routes.cart_url }}" class="btn btn-text" aria-label="{{ 'cart.title' | t }}">

                            <span class="site-mobile-header-item-counter{% if cart.items.size > 0 %} has-items{% endif %}">

                                <span class="site-mobile-header-item-counter-text" data-cart-item-count="true">
                                    {{ cart.item_count }}
                                </span>

                            </span>

                            {%- render 'icon', icon: 'cart', class: 'svg-icon-fw' -%}

                        </a>

                    </div>

                </div>

            </div>

        </div>

    </div>

</div>

<nav class="site-mobile-navigation-container{% if section.settings.mobileNavigationSimple %} site-mobile-navigation-simple{% endif %}">

    {% unless section.settings.mobileNavigationSimple %}

        {%- assign layerIndex = 0 -%}

        {% for item in mobileMenu.links %}

            {% if item.links %}

                {%- assign layerIndex = layerIndex | plus: 1 -%}

                <div class="site-mobile-navigation-layer site-mobile-navigation-layer-child" data-mobile-navigation-layer-id="{{ layerIndex }}">

                    <div class="site-mobile-navigation-header">

                        <div class="site-mobile-navigation-header-inner">

                            <button
                                type="button"
                                name="site-mobile-navigation-hide-layer"
                                class="btn btn-text"
                                value="{{ layerIndex }}">
                                {%- render 'icon', icon: 'angle-left' -%}
                            </button>

                            <span class="site-mobile-navigation-header-text">
                                {{ item.title }}
                            </span>

                        </div>

                    </div>

                    <ul class="site-mobile-navigation-menu">

                        {% for item in item.links %}

                            <li class="site-mobile-navigation-menu-item{% if item.links %} site-mobile-navigation-menu-item-parent{% endif %}{% if item.active %} site-mobile-navigation-menu-item-active{% endif %}">

                                {%- if item.url -%}

                                    <a
                                        href="{{ item.url }}"
                                        target="{{ item.target | default: '_self' }}"
                                        class="site-mobile-navigation-menu-item-text">
                                        {{ item.title }}
                                    </a>

                                    {%- if item.links -%}

                                        <button
                                            type="button"
                                            name="site-mobile-navigation-show-layer"
                                            class="btn btn-text"
                                            value="{{ layerIndex }}-{{ forloop.index }}">
                                            {%- render 'icon', icon: 'angle-right' -%}
                                        </button>

                                    {% endif %}

                                {%- else -%}

                                    <span class="site-mobile-navigation-menu-item-text">
                                        {{ item.title }}
                                    </span>

                                    {%- if item.links -%}

                                        <button
                                            type="button"
                                            name="site-mobile-navigation-show-layer"
                                            class="btn btn-text"
                                            value="{{ layerIndex }}-{{ forloop.index }}">
                                            {%- render 'icon', icon: 'angle-right' -%}
                                        </button>

                                    {% endif %}

                                {%- endif -%}

                            </li>

                        {% endfor %}

                    </ul>

                </div>

                {% for item in item.links %}

                    {% if item.links %}

                        <div class="site-mobile-navigation-layer site-mobile-navigation-layer-child" data-mobile-navigation-layer-id="{{ layerIndex }}-{{ forloop.index }}">

                            <div class="site-mobile-navigation-header">

                                <div class="site-mobile-navigation-header-inner">

                                    <button
                                        type="button"
                                        name="site-mobile-navigation-hide-layer"
                                        class="btn btn-text"
                                        value="{{ layerIndex }}-{{ forloop.index }}">
                                        {%- render 'icon', icon: 'angle-left' -%}
                                    </button>

                                    <span class="site-mobile-navigation-header-text">
                                        {{ item.title }}
                                    </span>

                                </div>

                            </div>

