                                    {%- render 'section-quick-link', style: section.settings.btnStyle, text: item.settings.btnLink.text -%}

                                </div>

                            {%- endif -%}

                        </div>
                    
                    </div>

                    {%- if item.settings.btnLink.url %}
                        </a>
                    {%- endif %}
                
                </div>

            {% endfor %}

        </div>

    {%- endif -%}

{%- endcapture -%}

