            {% if section.settings.sectionPaddingBottom == 0 %}
                #finqu-section-{{ section.id }} + .section-promotion-collage {
                    --section-padding-top: var(--style-grid-gutter-width);
                }
            {%- endif -%}
            {%- for item in section.settings.setting_blocks.items -%}
                {%- assign colorShemeBrightness = item.settings.colorScheme | color_brightness -%}
                {%- if colorShemeBrightness > 190 -%}
                    {%- assign accentTextColor = '#000' -%}
                {%- else -%}
                    {%- assign accentTextColor = '#fff' -%}
                {%- endif -%}
                #finqu-section-{{ section.id }} .section-content .collage-grid > div:nth-child({{ forloop.index}}) {
                    --style-heading-color: {{ item.settings.colorScheme }};
                    --style-subheading-color: {{ item.settings.colorScheme }};
                    --style-overline-color: {{ item.settings.colorScheme }};
                    --style-text-color: {{ item.settings.colorScheme }};
                    --style-link-color: {{ item.settings.colorScheme }};
                    --style-icon-color: {{ item.settings.colorScheme }};
                    --style-btn-bg-color: {{ item.settings.colorScheme }};
                    --style-btn-text-color: {{ accentTextColor }};
                    --style-btn-border-color: {{ item.settings.colorScheme }};
                    --style-border-color: {{ accentTextColor | color_mix: item.settings.colorScheme, 15 }};
                    --style-border-focus-color: {{ accentTextColor | color_mix: item.settings.colorScheme, 20 }};
                    --style-border-accent-color: {{ accentTextColor | color_mix: item.settings.colorScheme, 50 }};
                    --style-accent-color: {{ item.settings.colorScheme }};
                    --style-accent-text-color: {{ accentTextColor }};
