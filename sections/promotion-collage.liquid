                                {%- endcapture -%}

                                {% render 'placeholder-svg', type: 'image', class: class %}
                    
                            {%- endif -%}

                        </div>

                    </div>

                    <div class="grid g-0 position-absolute top-0 start-0 w-100 h-100 p-g p-md-5{%- if section.settings.imgDimming != '0' %} z-1{%- endif -%}" style="--bs-gap: 0">

                        <div class="g-col-12 place-content-{{ section.settings.textVerticalAlign }}">

                            {%- assign sectionHandle = '#finqu-section-' | append: section.id -%}

                            {% render 'section-header', sectionTitle: item.settings.title, sectionTitleTag: 'span', sectionDescription: item.settings.subtitle, sectionSubtitleTag: 'span', sectionOverline: item.settings.overline, sectionOverlineTag: 'span', sectionHeaderContainer: 'none', sectionAnimation: settings.sectionAnimation, sectionHandle: sectionHandle, sectionHeaderGapScale: 0.5, sectionHeaderTextAlign: textHorizontalAlign %}

