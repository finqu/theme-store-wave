            {% when '16:9' %}
                {%- assign imgSizeSm = '420,240' -%}
                {%- assign imgSizeMd = '370,212' -%}
                {%- assign imgSizeLg = '430,246' -%}
            {% when '4:3' %}
                {%- assign imgSizeSm = '420,317' -%}
                {%- assign imgSizeMd = '370,280' -%}
                {%- assign imgSizeLg = '430,325' -%}
            {% when '1:1' %}
                {%- assign imgSizeSm = '420,420' -%}
                {%- assign imgSizeMd = '370,370' -%}
                {%- assign imgSizeLg = '430,430' -%}
