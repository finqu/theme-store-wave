                        --style-btn-border-hover-color: {{ item.settings.colorScheme | color_lighten: 8 }};
                        --style-btn-border-active-color: {{ item.settings.colorScheme | color_lighten: 13 }};
                    {%- endif -%}
                }
            {%- endfor -%}
            {%- if section.settings.imgDimming != '0' -%}
                #finqu-section-{{ section.id }} .collage-grid .aspect-ratio-container::after {
                    content: '';
                    position: absolute;
                    left: 0;
                    top: 0;
                    right: 0;
                    bottom: 0;
                    z-index: 1;
